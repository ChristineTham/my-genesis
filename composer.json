{
	"name": "ChristineTham/my-genesis",
	"type": "wordpress-theme",
	"description": "Chris Tham personal website child theme for the Genesis Framework",
	"homepage": "https://github.com/ChristineTham/my-genesis/",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "HelloTham",
			"email": "info@hellotham.com"
		}
	],
	"require": {
		"php": "^5.2 || ^7",
		"composer/installers": "^1"
	},
	"require-dev": {
		"php": "^5.6 || ^7",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
		"phpcompatibility/phpcompatibility-wp": "^2",
		"squizlabs/php_codesniffer": "^3.3.2",
		"wp-coding-standards/wpcs": "^1.1"
	},
	"config": {
		"sort-order": true
	},
	"scripts": {
		"install-codestandards": "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run",
		"phpcs": "phpcs",
		"phpcbf": "phpcbf"
	},
	"support": {
		"issues": "https://github.com/ChristineTham/my-genesis/issues",
		"source": "https://github.com/ChristineTham/my-genesis/"
	}
}